<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".BackgroundActivity">

    <ImageView
            android:layout_width="122dp"
            android:layout_height="122dp"
            android:background="@drawable/ic_local_cafe_black_24dp"
            android:id="@+id/imageView2"
            android:layout_marginEnd="40dp"
            android:layout_marginTop="10dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentTop="true"/>
    <ImageView
            android:layout_width="122dp"
            android:layout_height="122dp"
            android:background="@drawable/ic_local_cafe_black_24dp"
            android:id="@+id/imageView3"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:layout_below="@id/imageView2"/>

    <ImageView
            android:layout_width="122dp"
            android:layout_height="122dp"
            android:background="@drawable/ic_memory_black_24dp"
            android:id="@+id/imageView4"
            android:layout_marginTop="10dp"
            android:layout_below="@+id/imageView2"
            android:layout_toEndOf="@+id/imageView3"/>
    <ImageView
            android:layout_width="122dp"
            android:layout_height="122dp"
            android:background="@drawable/ic_wb_cloudy_black_24dp"
            android:id="@+id/imageView5"
            android:layout_marginTop="10dp"
            android:layout_below="@+id/imageView2"
            android:layout_toEndOf="@+id/imageView4"/>
    <ImageView
            android:layout_width="122dp"
            android:layout_height="122dp"
            android:background="@drawable/ic_memory_black_24dp"
            android:id="@+id/imageView6"
            android:layout_below="@+id/imageView3"
            android:layout_alignEnd="@+id/imageView3"/>
    <ImageView
            android:layout_width="122dp"
            android:layout_height="122dp"
            android:background="@drawable/ic_wb_cloudy_black_24dp"
            android:id="@+id/imageView7"
            android:layout_below="@+id/imageView3"
            android:layout_toEndOf="@+id/imageView3"/>
    <ImageView
            android:layout_width="122dp"
            android:layout_height="122dp"
            android:background="@drawable/ic_local_cafe_black_24dp"
            android:id="@+id/imageView8"
            android:layout_below="@+id/imageView3"
            android:layout_toEndOf="@+id/imageView4"/>
    <Button
            android:text="save"
            android:layout_width="379dp"
            android:layout_height="40dp"
            android:id="@+id/btnsaveback"
            android:layout_centerHorizontal="true"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="90dp"/>
</RelativeLayout>